{
  "name": "@fluggo/graphlib-dot",
  "version": "0.6.5-pre",
  "description": "A DOT language parser / writer for graphlib",
  "author": "Chris Pettitt <cpettitt@gmail.com>",
  "main": "index.js",
  "keywords": [
    "graph",
    "dot",
    "parse"
  ],
  "scripts": {
    "generate-parser": "peggy --plugin ts-pegjs --extra-options-file src/pegconfig.json -o lib/dot-grammar.ts src/dot-grammar.pegjs"
  },
  "dependencies": {
    "graphlib": "2.1.8",
    "lodash": "4.17.19"
  },
  "devDependencies": {
    "peggy": "^1.2.0",
    "ts-pegjs": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fluggo/graphlib.git"
  },
  "contributors": [
    "Dominic Tarr <dominic.tarr@gmail.com> (http://bit.ly/dominictarr)"
  ],
